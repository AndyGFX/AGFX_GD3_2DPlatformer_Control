config_version=3

[application]

config/name="AGFX_G3_Platform_Engine"
run/main_scene="res://main.tscn"
config/icon="res://icon.png"

[autoload]

Globals="*res://Scripts/Globals.gd"
Utils="*res://Scripts/Utils/Utils.gd"
GameData="*res://Scripts/Utils/GameData.gd"

[display]

window/size/width=320
window/size/height=200
window/size/test_width=1280
window/size/test_height=800
window/stretch/mode="2d"
window/stretch/aspect="keep_width"

[editor_plugins]

enabled=PoolStringArray( "TileSetCutter", "aseprite-import", "ch.fischspiele.tilesethelper", "godotp-folder-structure", "vnen.tiled_importer" )

